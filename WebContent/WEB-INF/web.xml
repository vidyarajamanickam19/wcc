<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xmlns="http://java.sun.com/xml/ns/javaee" 
			xmlns:web="http://java.sun.com/xml/ns/javaee" 
			 
			xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
			 version="3.0">
  <welcome-file-list>
    <welcome-file>html/quality_report_view.html</welcome-file>
  </welcome-file-list>
   <servlet>
    <servlet-name>SystemVariables</servlet-name>
    <servlet-class>com.hpe.nca.wsqc.controller.SystemVariables</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SystemVariables</servlet-name>
    <url-pattern>/getenv</url-pattern>
  </servlet-mapping>
  
</web-app>